    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Server Node",
            "request": "launch",
            "type": "node",
            "cwd": "${workspaceFolder}\\system\\server\\",
            "program": "${workspaceFolder}\\system\\server\\server.js"
        },
        {
            "name": "Launch Script GenerateSystem",
            "request": "launch",
            "type": "node",
            "cwd": "${workspaceFolder}\\system\\script\\",
            "program": "${workspaceFolder}\\system\\script\\generateSystem.js"
        },
        {
            "name": "Attach by Process ID",
            "processId": "8",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        }
    ]
}